{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "03-ui-componentes-estilos": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular/build:application",
          "options": {
            "browser": "src/main.ts",
            "outputPath": "dist/icc-ppw-u3-estilos-componentes",
            "tsConfig": "tsconfig.app.json",

            "assets": [
              {
                "glob": "**/*",
                "input": "public",
                "output": "/"
              },
              {
                "glob": "**/*",
                "input": "src/assets",
                "output": "assets"
              }
            ],

            "styles": [
              "src/styles.css",
              "node_modules/ngx-toastr/toastr.css"
            ]
          },

          "configurations": {
            "production": {
              "baseHref": "/icc-ppw-u3-estilos-componentes/",
              "deployUrl": "/icc-ppw-u3-estilos-componentes/",
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kB",
                  "maximumError": "1MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "4kB",
                  "maximumError": "8kB"
                }
              ],
              "outputHashing": "all"
            },

            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.development.ts"
                }
              ]
            }
          },

          "defaultConfiguration": "production"
        },

        "serve": {
          "builder": "@angular/build:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "03-ui-componentes-estilos:build:production"
            },
            "development": {
              "buildTarget": "03-ui-componentes-estilos:build:development"
            }
          },
          "defaultConfiguration": "development"
        },

        "extract-i18n": {
          "builder": "@angular/build:extract-i18n"
        },

        "test": {
          "builder": "@angular/build:karma",
          "options": {
            "tsConfig": "tsconfig.spec.json",

            "assets": [
              {
                "glob": "**/*",
                "input": "public",
                "output": "/"
              },
              {
                "glob": "**/*",
                "input": "src/assets",
                "output": "assets"
              }
            ],

            "styles": [
              "src/styles.css"
            ]
          }
        },

        "deploy": {
          "builder": "angular-cli-ghpages:deploy"
        }
      }
    }
  },

  "cli": {
    "analytics": "d274b2f7-131c-4a6a-af3b-f766669ac004"
  }
}
